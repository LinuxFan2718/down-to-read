{
  "manifest_version": 2,
  "name": "Down To Read",
  "version": "1.0",
  "description": "Virtual Bookshelf of Your Kindle Library.",
  "browser_action": {
    "default_icon": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-2.2.4.min.js", "read-books.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "style.css",
    "wood.png",
    "border.png"
  ],
  "permissions": ["tabs"]
}
